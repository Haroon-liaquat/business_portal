<?php

/**
 * @file
 * Contains jazz_payment_section_3.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function jazz_payment_section_3_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the jazz_payment_section_3 module.
    case 'help.page.jazz_payment_section_3':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('My Awesome Module') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function jazz_payment_section_3_theme() {
  return [
    'jazz_payment_section_3' => [
      'render element' => 'children',
      'variables' =>
      [
        'data' => []
      ]
    ],
  ];
}
